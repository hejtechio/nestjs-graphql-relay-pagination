    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "@nestjs/graphql": "^12.0.0 || ^13.0.0",
    "@nestjs/typeorm": "^10.0.0 || ^11.0.0",
    "graphql": "^16.0.0",
    "reflect-metadata": "^0.2.0",
    "typeorm": "^0.3.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sqlite3"
