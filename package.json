{
  "name": "nestjs-graphql-relay-pagination",
  "version": "1.0.0",
  "description": "A flexible pagination module for NestJS applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand --testRegex=.*\\.int-spec\\.ts$",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm test && pnpm run lint",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "keywords": [
    "nestjs",
    "pagination",
    "cursor",
    "relay",
    "graphql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "class-validator": "0.14.1",
    "date-fns": "^2.30.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@dword-design/eslint-plugin-import-alias": "^3.0.0",
    "@foxglove/eslint-plugin": "^1.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/testing": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@stylistic/eslint-plugin-js": "^1.5.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cockroachdb": "^1.0.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-max-params-no-constructor": "^0.0.4",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "graphql": "^16.9.0",
    "jest": "^29.5.0",
    "prettier": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "0.3.20",
    "typescript": "^5.0.4",
    "vitest": "3.1.1"
  }
}
