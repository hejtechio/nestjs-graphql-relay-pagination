    "@foxglove/eslint-plugin": "^1.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/testing": "^10.0.0",
