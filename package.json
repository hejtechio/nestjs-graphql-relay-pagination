{
  "name": "@hejtech/nestjs-graphql-relay-pagination",
  "version": "0.0.1",
  "description": "A flexible pagination module for NestJS applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run && pnpm test:int",
    "test:watch": "vitest",
    "test:cov": "vitest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand --testRegex=.*\\.int-spec\\.ts$",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm test && pnpm run lint",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pretest:int": "docker-compose up -d test-db",
    "test:int": "jest --testTimeout=15000",
    "posttest:int": "docker-compose rm -sf test-db"
  },
  "keywords": [
    "nestjs",
    "pagination",
    "cursor",
    "relay",
    "graphql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "pg": "^8.12.0",
    "@dword-design/eslint-plugin-import-alias": "^5.0.0",
    "@foxglove/eslint-plugin": "^1.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/testing": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@stylistic/eslint-plugin-js": "^2.6.4",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cockroachdb": "^1.0.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-max-params-no-constructor": "^0.0.4",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "graphql": "^16.9.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.7.2",
    "vitest": "^2.0.5"
  }
}
